  <arg name="esdf_collision" 		value="2.0" />
  <arg name="frame" 			value="world" />   
  <arg name="map_resolution" 		value="0.1" />
  <arg name="start_x" 			value="-19.95" />
  <arg name="start_y" 			value="19.95" />  
  <arg name="safe_distance" 		value="1.0" />
