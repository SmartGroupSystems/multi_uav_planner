<launch>
 <!--轨迹优化-->
    <arg name="_map_size_x" value="20.0"/>
    <arg name="_map_size_y" value="20.0"/>
    <arg name="_map_size_z" value=" 5.0"/>
  <include file="$(find multi_bspline_opt)/launch/multi_bspline.launch">
    <arg name="uav_id_" value="0"/>
